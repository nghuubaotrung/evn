"return" 2>&- || "exit"
syntax on
filetype indent on
set autoindent
set number
colorscheme elflord
set nobackup

set runtimepath+=~/.vim/bundle/neobundle.vim/
 
" Required:
call neobundle#begin(expand('~/.vim/bundle/'))
 
NeoBundleFetch 'Shougo/neobundle.vim'
NeoBundle 'scrooloose/nerdtree'
 
 
call neobundle#end()
 
" Required:
filetype plugin indent on
 
NeoBundleCheck
